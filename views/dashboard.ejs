
<%- include('./partials/head', {current:'dashboard'})%>
<%- include('partials/nav') %>

<main>
    <header>
        <h1>Dashboard</h1>
        <p>Let's check your progress</p>   
    </header>

    <section>
        <article>
            <h3><%= company.name %></h3>
            <p>Deelgenomen januari X</p>
