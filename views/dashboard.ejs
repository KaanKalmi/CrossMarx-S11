            <img src="#" alt="profiel">
        </article>
        <article>
            <div><img src="#" alt="nieuw"></div>
            <p>Nieuw</p>
        </article>
    </section>

    <section class="set-up">
        <h2>Begin met opzetten</h2>
        <label> Beginner
            <progress id="file" value="32" max="100"> 32% </progress>
        </label>
